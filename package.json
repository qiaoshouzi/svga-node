{
  "name": "svga-node",
  "version": "1.0.2",
  "description": "Convert SVGA to image sequences in Node.js",
  "type": "module",
  "author": "qiaoshouzi <qiaoshouzi@cfm.moe>",
  "license": "MIT",
  "scripts": {
    "start": "node --enable-source-maps --unhandled-rejections=strict dist/index.js",
    "dev": "tsx --inspect src/test.ts",
    "build": "tsx build.ts --prod && tsc"
  },
  "dependencies": {
    "canvas": "^3.2.1",
    "protobufjs": "^8.0.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^25.2.3",
    "esbuild": "^0.27.3",
    "esbuild-plugin-tsconfig-paths": "^1.0.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "undici": "^7.21.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "keywords": [
    "svga",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiaoshouzi/svga-node"
  },
  "bugs": {
    "url": "https://github.com/qiaoshouzi/svga-node/issues"
  },
  "homepage": "https://github.com/qiaoshouzi/svga-node/#readme",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
